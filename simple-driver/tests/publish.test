<launch>

  <node name="Aanderaa4Optode" type="aanderaa4_driver_node.py" pkg="aanderaao2driver">
    <param name="device" value="/dev/aanderaa" />
    <param name="sample_interval" type="double" value="2" />
  </node>
  <node name="Aanderaa4Skill" type="aanderaa4_control_node.py" pkg="aanderaao2driver">
  </node>

  <test name="publishtest"
        test-name="publishtest"
        pkg="rostest" type="publishtest">
    <rosparam>
      topics:
        - name: /aanderaao2tests/o2/AanderaaO24x
          timeout: 5
          negative: False
        - name: /aanderaao2tests/o2/AanderaaO24x/standard
          timeout: 5
          negative: False
    </rosparam>
  </test>

</launch>